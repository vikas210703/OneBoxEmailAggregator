{
  "name": "email-onebox",
  "version": "1.0.0",
  "description": "Feature-rich email onebox aggregator with AI-powered categorization and real-time sync",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "import-netlify-env": "bash ./scripts/netlify/import_envs.sh",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "test": "jest"
  },
  "keywords": [
    "email",
    "imap",
    "elasticsearch",
    "ai",
    "onebox"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "@google/generative-ai": "^0.1.3",
    "@slack/webhook": "^7.0.2",
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "hnswlib-node": "^1.4.2",
    "imap": "^0.8.19",
    "mailparser": "^3.6.5",
    "natural": "^8.1.0",
    "openai": "^4.20.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/imap": "^0.8.40",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
